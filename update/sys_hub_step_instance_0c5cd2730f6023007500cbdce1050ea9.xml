        <value>(function execute(inputs, outputs) {

  var answer = 0;
  var op = inputs.operation.toString();
  var number1 = parseInt(inputs.number1);
  var number2 = parseInt(inputs.number2);

  switch (op) {
    case 'add':
      answer = number1 + number2;
      break;
    case 'subtract':
      answer = number1 - number2;
      break;
    case 'multiply':
      answer = number1 * number2;
      break;
    case 'divide':
      answer = number1 / number2;
      break;
    case 'modulo':
      answer = number1 % number2;
      break;

  }
  gs.info('&gt;&gt;&gt;DEBUG: ' + number1 + ' ' + op + ' ' + number2 + '=' + answer);
  outputs.result = answer;
  
})(inputs, outputs);
</value>
